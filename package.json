{
  "name": "sport-visioner",
  "version": "0.0.1-0",
  "description": "A sample application based on the Decathlon Sport Vision API",
  "author": "Thomas Vanier <thomas.vanier@gmail.com>",
  "scripts": {
    "lint": "standard src *.js",
    "dev": "vite",
    "test": "jest src",
    "test:watch": "npm run test -- --watch",
    "build": "vite build",
    "serve": "vite preview"
  },
  "devDependencies": {
    "@babel/preset-env": "7.16.4",
    "@testing-library/jest-dom": "5.15.0",
    "@testing-library/svelte": "3.0.3",
    "canvas": "2.8.0",
    "eslint": "7.32.0",
    "jest": "27.3.1",
    "jest-svelte-events": "1.0.5",
    "rollup-plugin-svelte": "7.1.0",
    "standard": "16.0.4",
    "svelte": "3.44.2",
    "svelte-jester": "1.6.0",
    "svelte-select": "3.17.0",
    "vite": "2.6.14"
  },
  "dependencies": {
    "@vtmn/css-button": "0.8.2",
    "@vtmn/css-design-tokens": "0.9.1",
    "typeface-roboto": "1.1.13",
    "typeface-roboto-condensed": "1.1.13"
  },
  "standard": {
    "globals": [
      "fetch"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": "svelte-jester"
    },
    "moduleFileExtensions": [
      "js",
      "svelte"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect",
      "jest-svelte-events/extend-expect"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "resources": "usable"
    }
  }
}
